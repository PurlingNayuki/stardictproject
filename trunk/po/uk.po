msgid "Custom Font"
msgstr "Шрифт користувача"

#: ../data/stardict.schemas.in.h:3
msgid "Dictionary configuration XML"
msgstr "XML з конфігурацією словника"

#: ../data/stardict.schemas.in.h:4
msgid "Dictionary order"
